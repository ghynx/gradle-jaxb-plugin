<xsd:schema elementFormDefault="qualified" jxb:version="2.1"
            xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
            xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            jxb:extensionBindingPrefixes="xjc">
	<xsd:annotation>
		<xsd:appinfo>
				<jxb:globalBindings generateValueClass="true" typesafeEnumMaxMembers="0">
					<xjc:javaType name="java.util.Calendar"
                        xmlType="xsd:date"
                        adapter="no.entitas.gradle.jaxb.samples.util.JaxbCalendarUtil" />
				</jxb:globalBindings>
      <jxb:schemaBindings>
        <jxb:package name="no.entitas.gradle.jaxb.samples.api"/>
      </jxb:schemaBindings>
    </xsd:appinfo>
	</xsd:annotation>
	<xsd:complexType name="complex">
		<xsd:sequence>
			<xsd:element name="el1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="name" type="xsd:string" />
						<xsd:element name="date" type="xsd:date" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="el2">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="name" type="xsd:string" />
						<xsd:element name="date" type="xsd:date" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>

